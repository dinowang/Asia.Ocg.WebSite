{
  "name": "asiaocg",
  "version": "0.0.1",
  "description": "http://xpg.cards",
  "scripts": {
    "remove-demo": "babel-node tools/removeDemo.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm run clean-dist && npm run build:html && npm run lint && npm run test",
    "build": "babel-node tools/build.js && npm run open:dist",
    "test": "mocha tools/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:watch": "npm run test -- --watch",
    "static-file": "babel-node tools/build.js && npm run build:html",
    "prod": "better-npm-run start-prod",
    "rickbuild": "better-npm-run build",
    "deploy": "npm run rickbuild && npm run prod"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "author": "ChRick",
  "dependencies": {
    "babel-polyfill": "6.9.1",
    "babel-preset-stage-2": "6.11.0",
    "better-npm-run": "0.0.9",
    "es6-promise": "3.2.1",
    "express": "4.14.0",
    "isomorphic-fetch": "2.2.1",
    "moment": "2.13.0",
    "object-assign": "4.1.0",
    "piping": "1.0.0-rc.2",
    "react": "15.0.2",
    "react-datepicker": "0.27.0",
    "react-dom": "15.0.2",
    "react-ga": "2.1.0",
    "react-redux": "4.4.5",
    "react-router": "2.4.0",
    "react-router-redux": "4.0.5",
    "redux": "3.5.2",
    "redux-async-connect": "1.0.0-rc4",
    "redux-connect": "2.4.0",
    "redux-thunk": "2.1.0",
    "serialize-javascript": "1.3.0",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.5.0",
    "babel-preset-stage-3": "^6.11.0",
    "babel-register": "6.8.0",
    "browser-sync": "2.12.5",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.20.0",
    "clean-webpack-plugin": "0.1.9",
    "compression": "1.6.2",
    "connect-history-api-fallback": "1.2.0",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "enzyme": "2.2.0",
    "eslint": "2.9.0",
    "eslint-config-airbnb": "8.0.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-jsx-a11y": "1.0.4",
    "eslint-plugin-react": "5.0.1",
    "eslint-watch": "2.1.11",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "font-awesome": "4.6.3",
    "mocha": "2.4.5",
    "node-sass": "3.7.0",
    "npm-run-all": "1.8.0",
    "pretty-error": "2.0.0",
    "react": "15.0.2",
    "react-addons-test-utils": "15.0.2",
    "react-helmet": "3.1.0",
    "redux-actions": "0.9.1",
    "redux-immutable-state-invariant": "1.2.3",
    "rimraf": "2.5.2",
    "sass-loader": "3.2.0",
    "serve-favicon": "2.3.0",
    "sinon": "1.17.4",
